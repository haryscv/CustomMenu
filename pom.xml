     <dependency>
      <groupId>javax.xml.bind</groupId>
      <artifactId>jaxb-api</artifactId>
       <scope>system</scope>
      <version>2.3.0</version>
       <systemPath>${project.basedir}/src/lib/jaxb-api-2.3.0.jar</systemPath>
