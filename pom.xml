      <version>1.7.25</version>
      <systemPath>${project.basedir}/src/lib/slf4j-api-1.7.25.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.emc.d2</groupId>
      <artifactId>X3-Common</artifactId>
      <scope>system</scope>
      <version>${documentum-D2.version}</version>
      <systemPath>${project.basedir}/src/lib/X3-Common-20.4.0000.0091.jar</systemPath>
