      <artifactId>D2-Constants</artifactId>
      <scope>system</scope>
      <version>${documentum-D2.version}</version>
      <systemPath>${project.basedir}/src/lib/D2-Constants-20.4.0000.0091.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.emc.d2</groupId>
      <artifactId>D2FS</artifactId>
      <scope>system</scope>
      <version>${documentum-D2.version}</version>
      <systemPath>${project.basedir}/src/lib/D2FS-20.4.0000.0091.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.emc.d2</groupId>
      <artifactId>D2FS-API</artifactId>
      <scope>system</scope>
      <version>${documentum-D2.version}</version>
      <systemPath>${project.basedir}/src/lib/D2FS-API-20.4.0000.0091.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.emc.d2</groupId>
      <artifactId>D2FS-Generated</artifactId>
