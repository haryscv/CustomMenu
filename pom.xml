    <dependency>
      <groupId>com.sun.xml.bind</groupId>
      <artifactId>jaxb-core</artifactId>
      <scope>system</scope>
      <version>2.3.0</version>
      <systemPath>${project.basedir}/src/lib/jaxb-core-2.3.0.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.sun.xml.bind</groupId>
      <artifactId>jaxb-impl</artifactId>
      <scope>system</scope>
      <version>2.3.0</version>
      <systemPath>${project.basedir}/src/lib/jaxb-impl-2.3.0.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.sun.xml.messaging.saaj</groupId>
      <artifactId>saaj-impl</artifactId>
      <scope>system</scope>
      <version>1.5.0</version>
      <systemPath>${project.basedir}/src/lib/saaj-impl-1.5.0.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>javax.xml.ws</groupId>
      <artifactId>jaxws-api</artifactId>
      <scope>system</scope>
      <version>2.2.6</version>
      <systemPath>${project.basedir}/src/lib/jaxws-api-2.2.6.jar</systemPath>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <!-- Make this jar executable -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <version>3.0.2</version>

        <configuration>
          <!-- DO NOT include log4j.properties file in your Jar -->
          <archive>
            <manifestEntries>
              <Specification-Title>Custom D2 Plugin</Specification-Title>
              <Specification-Vendor>Hareesh</Specification-Vendor>
               <Built-By>${user.name}</Built-By>
              <Build-Date>${maven.build.timestamp}</Build-Date>
              <Compile-Version>${maven.compiler.target}</Compile-Version>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
    </plugins>
  </build>

</project>
